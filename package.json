{
	"name": "template-javy-typescript-hello-world",
	"version": "1.0.0",
	"description": "Javy typescript template for the Blockless CLI.",
	"main": "index.js",
	"scripts": {
		"clean": "rm -rf build",
		"build:release": "mkdir -p ./build/ && bls-sdk-ts build ./index.ts -o ./build -f {name}.wasm",
		"build:debug": "mkdir -p ./build/ && bls-sdk-ts build ./index.ts -o ./build -f {name}.wasm",
		"invoke": "bls-runtime ./build/{name}.wasm"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/blocklessnetwork/template-javy-typescript-hello-world.git"
	},
	"author": "Blockless",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/blocklessnetwork/template-javy-typescript-hello-world/issues"
	},
	"homepage": "https://github.com/blocklessnetwork/template-javy-typescript-hello-world#readme",
	"dependencies": {
		"@blockless/sdk-ts": "^1.0.2"
	}
}
